    think setq(3, null() );
    think setq( C, null() );
    think setq( L, 0 );
    think iter( r( A ), [ifelse( match( r( C ), before( itext( 0 ), : ) ), setq( r( L ), cat( r( r( L ) ), itext( 0 ) ) ), [setq( C, before( itext( 0 ), : ) )][ifelse( gt( inum( 0 ), mul( r( L ), fdiv( words( r( A ) ), 3 ) ) ), [setq( L, inc( r( L ) ) )], )][setq( r( L ), cat( r( r( L ) ), itext( 0 ) ) )] )] );
    think setq( C, null() );
